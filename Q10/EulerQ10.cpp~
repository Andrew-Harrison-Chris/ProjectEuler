#include <iostream>

using namespace std;

bool isPrime(int n);

int main () {

  int n=10;
  int x=0;
  
  for (int i=2; i<=n; i++) {
    if (isPrime(i)) {
      x+=i;
    }
  }
  cout << x << endl;

  return 0;
}

bool isPrime(int n) {

  for (int i=2; i<n/2; i++) {
    if (n%i == 0) return false;
  }
  return true;
}
