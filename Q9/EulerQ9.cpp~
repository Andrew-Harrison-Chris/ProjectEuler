#include <iostream>

using namespace std;

int main () {

  for (int a=0; a<1000; a++) {
    for (int b=0; b<1000; b++) {
      for (int c=0; c<1000; c++) {
	if (a*a + b*b == c*c && a+b+c == 1000) {
	  cout << "The product of a, b, and c is: " << a*b*c << endl;
	  exit(0);
	}
      }
    }
  }

  return 0;
}
